<TelaMenu>:
    name: 'Menu'

    MDNavigationLayout:
        ScreenManager:
            Screen:
                name: 'menu_screen'
                MDBottomNavigation:
                    md_bg_color: [0.0, 0.0, 0.5, 1]
                    selected_color_background: app.theme_cls.primary_color
                    text_color_active: 0, 0, 0, 1
                    height: dp(64)
                    spacing: 5

                    MDBottomNavigationItem:
                        id: screen_1
                        name: 'screen 1'
                        text: 'Menu'
                        icon: 'home'
                        halign: 'center'
                        on_tab_press: app.switch_screen('menu_screen')
                        Image:
                            source: 'BackgroundMenu.png'
                            allow_stretch: True
                            keep_ratio: False

                        FloatLayout:
                            size_hint: None, None
                            size: dp(200), dp(200)
                            pos_hint: {'center_x': 0.5, 'center_y': 0.7}

                            MDTextField:
                                hint_text: "Pesquisar"
                                mode: "round"
                                halign: 'left'
                                padding: 10
                                size_hint_x: None
                                width: dp(350)
                                pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                                icon_left: 'magnify'
                        BoxLayout:
                            orientation: 'vertical'
                            size_hint: (0.9, 0.1)  
                            pos_hint: {"center_x": 0.5, "center_y": 0.43}
                            AsyncImage:
                                source: 'web.png'
                                allow_stretch: True
                                keep_ratio: False
                                size_hint: (1, 1)
                                on_touch_down:
                                    if self.collide_point(*args[1].pos): root.open_website()

                        FloatLayout:
                            MDBoxLayout:
                                orientation: 'vertical'
                                padding: dp(16)
                                spacing: dp(16)
                                size_hint_y: None
                                height: self.minimum_height
                                pos_hint: {"center_x": 0.47, "top": 0.67} 

                                GridLayout:
                                    cols: 4
