                                            theme_text_color: "Custom"
                                            text_color: 1, 1, 1, 1
                                            pos_hint: {"center_x": .5, "center_y": .5}

                                    MDBoxLayout:
                                        orientation: 'vertical'
                                        size_hint: None, None
                                        size: dp(70), dp(70)
                                        md_bg_color: 0.2, 0.0, 0.6, 1
                                        radius: [12, 12, 12, 12]
                                        MDIconButton:
                                            icon: "chat"
