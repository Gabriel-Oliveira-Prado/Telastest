<SplashScreen>:
    name: 'splash'
    

<TelaEntrarLogin>:
    name: 'Entrar_login'
    background_color: '#FFFFFF'
    Image:
        source: 'WORKINbackgroundinicial.png'
        allow_stretch: True
        keep_ratio: False  
    MDLabel:
        theme_text_color: "Custom"
        text_color: '#FFFFFF'
        text: 'Mudar para'
        halign: "center"
        pos_hint: {'center_x': 0.5, 'center_y': 0.13}
    MDRectangleFlatIconButton:
        icon: 'account-tie'
        text: 'Pessoa jurídica'
        pos_hint: {'center_x': 0.5, 'center_y': 0.07}
        custom_color: "Gray"
        theme_text_color: "Custom"
        text_color: 0, 0, 0, 1 
        md_bg_color: "#FDFAF3"
        icon_color: 0, 0, 0, 1
        on_release: 
            app.root.transition.direction = "left"
            app.root.current = 'Entrar_Login_jurídico'
    MDCard:
        size_hint: None, None
        size: dp(260), dp(350) 
        pos_hint: {'center_x': 0.5, 'center_y': 0.6}
        elevation: 0
        orientation: 'vertical'
        padding: dp(15)
        spacing: dp(10)
        md_bg_color: 0, 0, 0, 0  
        radius: [dp(10)]  
        MDLabel:
            theme_text_color: "Custom"
            text_color: '#FFFFFF'
            text: "Login"
            font_size: dp(30)  
            halign: "center"
            size_hint_y: None
            height: self.texture_size[1]
            padding_y: dp(5)
        MDTextField:
            id: email
            hint_text: "Digite seu Email" 
            hint_text_color_normal: 0.85, 0.85, 0.85, 1
            text_color: 1, 1, 1, 1
            icon_right: "account"
            text_color_focus: 1, 1, 1, 1 
            icon_right_color_normal: 1, 1, 1, 1 
            size_hint_x: None
            width: dp(200)
            font_size: dp(14)  
            pos_hint: {'center_x': 0.5}
            mode: "rectangle"
            line_color_normal: "#FFFFFF" 
            border: [1, 1, 1, 1]
        MDTextField:
            id: senha
            password: True
            hint_text: "Digite sua Senha"
            hint_text_color_normal: 0.85, 0.85, 0.85, 1
            text_color: 1, 1, 1, 1
            icon_right: "account-key"
            text_color_focus: 1, 1, 1, 1 
            icon_right_color_normal: 1, 1, 1, 1  
            size_hint_x: None
            width: dp(200)  
            font_size: dp(14) 
            pos_hint: {'center_x': 0.5}
            mode: "rectangle"
            line_color_normal: "#FFFFFF"
            border: [1, 1, 1, 1]          
        MDTextButton:
            text: "Esqueceu a senha?"
            theme_text_color: "Custom"
            text_color: 1, 1, 1, 1 
            font_size: "12sp"
            pos_hint: {"center_x": 0.5}
            on_release: app.open_link("https://workinweb.netlify.app/")
        MDFloatLayout:
            spacing:10
            pos_hint: {'center_y': 0.5}
            MDRaisedButton:  
                text: "     Entrar     "
                custom_color: "Gray"
                theme_text_color: "Custom"
                text_color: 0, 0, 0, 1 
                md_bg_color: "#FDFAF3"
                font_size: dp(14) 
                pos_hint: {'center_x': 0.25, 'center_y': 0.65}
                on_release:
                    root.Login()
            MDRaisedButton:
                text: "Criar conta"
                custom_color: "Gray"
                theme_text_color: "Custom"
                text_color: 0, 0, 0, 1
                md_bg_color: "#FDFAF3"
                font_size: dp(14)
                pos_hint: {'center_x': 0.75, 'center_y': 0.65}
                on_release:
                    app.root.transition.direction = 'right'
                    app.root.current = 'Criar_conta'

