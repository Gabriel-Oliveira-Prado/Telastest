ScreenManager:
    MainScreen:

<MainScreen>:
    name: 'main'
    BoxLayout:
        orientation: 'vertical'

        ScreenManager:
            id: screen_manager

            Screen:
                name: 'screen_Contatos'
                BoxLayout:
                    orientation: 'vertical'
                    MDLabel:
                        text: 'Contatos'
                        halign: 'center'

            Screen:
                name: 'screen_Comunidades'
                BoxLayout:
                    orientation: 'vertical'
                    MDLabel:
                        text: 'Grupos'
                        halign: 'center'

            Screen:
                name: 'screen_Comunidades2'
                BoxLayout:
                    orientation: 'vertical'
                    MDLabel:
                        text: 'Comunidades'
                        halign: 'center'

        MDBottomNavigation:
            MDBottomNavigationItem:
                name: 'screen_Contatos'
                text: 'Contatos'
                icon: 'chat-processing-outline'
                on_tab_press: screen_manager.current = 'screen_Contatos'

            MDBottomNavigationItem:
                name: 'screen_Comunidades'
                text: 'Grupos'
                icon: 'text-box-multiple-outline'
                on_tab_press: screen_manager.current = 'screen_Comunidades'

            MDBottomNavigationItem:
                name: 'screen_Comunidades2'
                text: 'Comunidades'
                icon: 'account-group-outline'
                on_tab_press: screen_manager.current = 'screen_Comunidades2'